tau = 6.28318530717958647692
pi  = 3.14159265358979323846
e   = 2.71828182845904523536

Floor = (v) => {
    return v - (v % 1)
}

Ceil = (v) => {
    return v - (v % 1) + 1
}

Round = (v) => {
    return floor(v + 0.5)
}

Min = (a, b) => {
    if a < b {
        return a
    }
    return b
}

Max = (a, b) => {
    if a < b {
        return b
    }
    return a
}